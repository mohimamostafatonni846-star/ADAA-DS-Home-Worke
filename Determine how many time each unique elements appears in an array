#include <stdio.h>

int main() {
    int n, i, j, count;
    printf("Enter number of elements: ");
    scanf("%d", &n);
    int arr[n];
    
    printf("Enter %d elements: ", n);
    for(i = 0; i < n; i++)
        scanf("%d", &arr[i]);
    
    printf("Element occurrences:\n");
    for(i = 0; i < n; i++) {
        count = 1;
        if(arr[i] == -1) continue; // already counted
        for(j = i+1; j < n; j++) {
            if(arr[i] == arr[j]) {
                count++;
                arr[j] = -1; // mark as counted
            }
        }
        printf("%d occurs %d times\n", arr[i], count);
    }
    return 0;
}